{
  "extends": "eslint:recommended",
  "env": {
    "browser": false,
    "node": false,
    "commonjs": true
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "comma-dangle": [1, "never"],
    "indent": [1, 2],
    "linebreak-style": [2, "unix"],
    "quotes": [1, "double"],
    "semi": [2, "always"]
  }
}
