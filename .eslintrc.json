{
  "extends": "airbnb/base",
  "env": {
    "browser": false,
    "node": false,
    "commonjs": true
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "indent": [1, 2],
    "linebreak-style": [2, "unix"],
    "no-unused-expressions": [2, { "allowShortCircuit": true }]
  }
}
